   "version": "0.2.0",
   "configurations": [
      {
         "name": "(gdb) USB remote edison",
         "type": "cppdbg",
         "request": "launch",
         "program": "${workspaceRoot}/joystick3d",
         "miDebuggerServerAddress": "192.168.2.15:9091",
         "args": [],
         "stopAtEntry": false,
         "cwd": "${workspaceFolder}",
         "environment": [],
         "externalConsole": true,
         "MIMode": "gdb",
         "sourceFileMap": {
            "${workspaceRoot}/src":"/root/joystick3d/src",
         },
         "setupCommands": [
            {
               "description": "Enable pretty-printing for gdb",
               "text": "-enable-pretty-printing",
               "ignoreFailures": true
            }
         ]
      }
   ]
}