{
   // See https://go.microsoft.com/fwlink/?LinkId=733558
   // for the documentation about the tasks.json format
   "version": "2.0.0",
   "tasks": [
      {
         "label": "make",
         "type": "shell",
         "command": "~/edison/somax/tools/edison_cmd.exp make joystick3d",
         "options": {
            "cwd": "${workspaceRoot}"
         },
         "group": {
            "kind": "build",
            "isDefault": true
         },
         "problemMatcher": [
            "$gcc"
         ]
      },
      {
         "label": "make-clean",
         "type": "shell",
         "command": "~/edison/somax/tools/edison_cmd.exp make joystick3d clean",
         "group": {
            "kind": "build",
            "isDefault": true
         },
         "problemMatcher": [
            "$gcc"
         ]
      }
   ]
}